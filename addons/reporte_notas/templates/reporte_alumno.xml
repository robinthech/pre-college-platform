<odoo>

  <!-- REPORTE PARA EL ALUMNO : reporte DE NOTAS -->
  <template id="reporte_notas_alumno_form" name="Reporte">
    <t t-call="website.layout">
      <div class="wrapper">
        <!-- Sidebar -->
        <nav id="sidebar" style="background-color:#51a3f4;">

          <ul class="list-unstyled components">
            <p style="background-color:#012bbf; height:60px;" class="text-center">
              <strong>REPORTE DE NOTAS</strong>
            </p>

            <li>
              <a class="" href="/reporte/alumno">ACADEMICO DE ALUMNO</a>
            </li>
            <li>
              <a href="/reporte/alumno/asistencia">ASISTENCIA</a>
            </li>
          </ul>
        </nav>

        <!-- Page Content -->
        <div id="content">
          <button type="button" id="sidebarCollapse" class="btn btn-info">
            <i class="fa fa-align-left"></i>
          </button>
          <h4 class="text-center mt-5">REPORTE DE NOTAS:
            <span t-field="user.name"></span>
          </h4>
          <section class="search-section-reporte">
            <div class="align-items-center">
              <form action="#" method="post" novalidate="novalidate">
                <div class="row">
                  <div class="col-lg-12">
                    <div class="row text-center">
                      <div class="col-lg-3 col-md-3 col-sm-12 p-0">
                        <h6 class="text-center m-0 p-0" style="color:white;">DESDE</h6>
                        <input class="text-center m-0 p-0 w-50" t-att-value="fecha_inicio" name="fecha_desde" type="date" placeholder="dd/mm/aaaa"/>
                      </div>
                      <div class="col-lg-3 col-md-3 col-sm-12 p-0">
                        <h6 class="text-center m-0 p-0" style="color:white;">HASTA</h6>
                        <input class="text-center m-0 p-0 w-50" t-att-value="fecha_fin" name="fecha_hasta" type="date" placeholder="dd/mm/aaaa"/>
                      </div>
                      <div class="col-lg-3 col-md-3 col-sm-12 p-0">
                        <h6 class="text-center m-0 p-0" style="color:white;">AULA</h6>
                        <select class="text-center m-0 p-0 w-75" name="aula">
                          <t t-foreach="aulas" t-as="aula">
                            <option t-att-value="'%s' % (aula.id)" t-esc="aula.name"/>
                          </t>
                        </select>
                      </div>
                      <div class="col-lg-3 col-md-3 col-sm-12 p-0">
                        <button type="button" class="btn wrn-btn-reporte w-50 wrn-btn-reporte-admin">REPORTAR</button>
                      </div>
                    </div>
                  </div>
                </div>
              </form>
            </div>
          </section>
          <div id="pruenda-render">
            <div id="template-reporte-render"></div>
          </div>
          <div class="mx-auto align-items-center m-3 p-3" style="width:100%; background-color: white;">

            <br/>
            <div class="reporte" style="background-color: white;">
              <div class="align-items-center text-center">
                <h3>PRUEBAS DIARIAS</h3>
              </div>
              <br/>
              <div id="template-reporte-diario-render"></div>
              <br/>
              <div class="align-items-center text-center">
                <h3>PRUEBAS SEMANAL</h3>
              </div>
              <br/>

              <div id="template-reporte-semanal-render"></div>

              <br/>
              <div class="align-items-center text-center">
                <h3>PRUEBAS SIMULACRO</h3>
              </div>
              <br/>

              <div id="template-reporte-simulacro-render"></div>
              <br/>

              <canvas id="bar-chart" width="800" height="450"></canvas>
              <br/>
              <div id="template-reporte-situacion-render"></div>
              <br/>
            </div>

          </div>

        </div>
      </div>



    </t>
  </template>


