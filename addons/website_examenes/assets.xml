<odoo>
    <template id="assets_evaluaciones" inherit_id="website.assets_frontend">
        <xpath expr="script[last()]" position="after">
          <script src="/website_examenes/static/src/js/evaluaciones.js" type="text/javascript"/>
          <script src="/website_examenes/static/src/lib/notify.js" type="text/javascript"/>
        </xpath>
        <xpath expr="link[last()]" position="after">
          <link href="/website_examenes/static/src/scss/evaluaciones.scss" type="text/scss"  rel="stylesheet" />
          <link href="/website_examenes/static/src/scss/menu.scss" type="text/scss"  rel="stylesheet"/>
          <link href="/website_examenes/static/src/scss/examen.scss" type="text/scss"  rel="stylesheet"/>

        </xpath>
    </template>
    <template id="assets_backend" inherit_id="web.assets_backend" name="Evaluaciones Backend Assets">
        <xpath expr="." position="inside">
          <script type="text/javascript" src="/website_examenes/static/src/js/widget_cursos.js"/>
          <script type="text/javascript" src="/website_examenes/static/src/js/widget_cursos_general.js"/>
          <!--script type="text/javascript" src="/website_examenes/static/src/js/menu_web.js"/-->
        </xpath>
    </template>
</odoo>
